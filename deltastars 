// Utility functions
function generateUserId() {
    return 'user_' + Date.now() + '_' + Math.random().toString(36).substr(2, 9);
}

function saveUsers() {
    localStorage.setItem('deltaStarsUsers', JSON.stringify(users));
}

function checkAdminAccess() {
    // Check if current user has admin access
    if (currentUser && (currentUser.role === 'admin' || currentUser.role === 'developer')) {
        return true;
    }
    return false;
}

function upgradeToVIP() {
    if (!currentUser) return;
    
    if (currentUser.accountType === 'vip') {
        showNotification('ุฃูุช ุนุถู VIP ุจุงููุนู!', 'info');
        return;
    }
    
    // VIP upgrade logic (in real app, this would involve payment)
    if (confirm('ูู ุชุฑูุฏ ุงูุชุฑููุฉ ุฅูู ุนุถููุฉ VIPุ\nูุฒุงูุง ุงูุนุถููุฉ:\n- ุฎุตููุงุช ุญุตุฑูุฉ\n- ุชูุตูู ูุฌุงูู\n- ุฏุนู ุฃููููุฉ\n- ููุงุท ุฅุถุงููุฉ')) {
        currentUser.accountType = 'vip';
        currentUser.points += 500; // Bonus points for upgrading
        
        // Update in users array
        const userIndex = users.findIndex(u => u.id === currentUser.id);
        if (userIndex !== -1) {
            users[userIndex] = currentUser;
            saveUsers();
        }
        
        localStorage.setItem('deltaStarsCurrentUser', JSON.stringify(currentUser));
        
        showNotification('๐ ุชูุงูููุง! ุชูุช ุชุฑููุชู ุฅูู ุนุถููุฉ VIP!', 'success');
        
        // Refresh profile view
        setTimeout(() => {
            const modal = bootstrap.Modal.getInstance(document.querySelector('.modal.show'));
            if (modal) modal.hide();
            setTimeout(() => showProfile(), 300);
        }, 2000);
    }
}

