    // Prepare order data
    const deliveryInfo = JSON.parse(sessionStorage.getItem('checkoutDelivery') || '{}');
    const location = JSON.parse(sessionStorage.getItem('checkoutLocation') || '{}');
    const paymentMethod = document.querySelector('input[name="paymentMethod"]:checke
