Tool Call
Function Name:
Write
Arguments:
file_path:
/home/user/deltastars-enhanced/js/auth.js
content:
// DeltaStars Authentication & User Management

// Current user state
let currentUser = JSON.parse(localStorage.getItem('deltaStarsCurrentUser') || 'null');
let users = JSON.parse(localStorage.getItem('deltaStarsUsers') || '[]');

// Admin credentials
const ADMIN_CREDENTIALS = {
    email: 'deltastars777@gmail.com',
    phone: '+966501234567',
    defaultPassword: '12345',
    role: 'admin'
};

const DEVELOPER_CREDENTIALS = {
    email: 'deltastars777@gmail.com',
    defaultPassword: '12345',
    role: 'developer'
};

// Initialize authentication system
function initAuth() {
    updateUserInterface();
    checkAdminAccess();
    
    // Check if user is logged in
    if (currentUser) {
        showUserLoggedIn();
    }
    
    // Initialize biometric authentication if available
    initBiometricAuth();
}

// User registration
function showRegister() {
    // Hide other modals first
    const loginModal = bootstrap.Modal.getInstance(document.getElementById('loginModal'));
    if (loginModal) loginModal.hide();
    
    // Show register modal
    setTimeout(() => {
        new bootstrap.Modal(document.getElementById('registerModal')).show();
    }, 300);
}

function registerUser(userData) {
    // Check if user already exists
    const existingUser = users.find(user => 
        user.phone === userData.phone || user.email === userData.email
    );
    
    if (existingUser) {
        throw new Error('ุงููุณุชุฎุฏู ููุฌูุฏ ุจุงููุนู');
    }
    
    // Create new user
    const newUser = {
        id: generateUserId(),
        ...userData,
        accountType: 'regular', // regular, vip
        points: 0,
        joinedAt: new Date().toISOString(),
        isVerified: false,
        lastLogin: null,
        orderCount: 0,
        totalSpent: 0,
        preferences: {
            notifications: true,
            newsletter: true,
            sms: true
        },
        addresses: [],
        paymentMethods: []
    };
    
    // Add to users array
    users.push(newUser);
    saveUsers();
    
    return newUser;
}

// User login
function showLogin() {
    // Hide other modals first
    const registerModal = bootstrap.Modal.getInstance(document.getElementById('registerModal'));
    if (registerModal) registerModal.hide();
    
    // Show login modal
    setTimeout(() => {
        new bootstrap.Modal(document.getElementById('loginModal')).show();
    }, 300);
}

function loginUser(phone, password) {
    // Check admin credentials
    if (phone === ADMIN_CREDENTIALS.phone && password === ADMIN_CREDENTIALS.defaultPassword) {
        const adminUser = {
            id: 'admin',
            name: 'ูุฏูุฑ ุงููุธุงู',
            email: ADMIN_CREDENTIALS.email,
            phone: ADMIN_CREDENTIALS.phone,
            role: 'admin',
            accountType: 'admin'
        };
        
        setCurrentUser(adminUser);
        return adminUser;
    }
    
    // Check regular users
    const user = users.find(u => u.phone === phone);
    
    if (!user) {
        throw new Error('ุฑูู ุงููุงุชู ุบูุฑ ูุณุฌู');
    }
    
    if (user.password !== password) {
        throw new Error('ูููุฉ ุงููุฑูุฑ ุฎุงุทุฆุฉ');
    }
    
    // Update last login
    user.lastLogin = new Date().toISOString();
    saveUsers();
    
    setCurrentUser(user);
    return user;
}

// OTP verification
function sendOTP(phone) {
    // Generate random 6-digit OTP
    const otp = Math.floor(100000 + Math.random() * 900000).toString();
    
    // Store OTP temporarily (in real app, this would be sent via SMS)
    const otpData = {
        phone: phone,
        otp: otp,
        expiresAt: new Date(Date.now() + 5 * 60 * 1000).toISOString() // 5 minutes
    };
    
    sessionStorage.setItem('currentOTP', JSON.stringify(otpData));
    
    // In development, show OTP in console
    console.log(`OTP for ${phone}: ${otp}`);
    
    // Show notification with OTP (for development only)
    showNotification(`ุฑูุฒ ุงูุชุญูู: ${otp} (ุตุงูุญ ููุฏุฉ 5 ุฏูุงุฆู)`, 'info');
    
    return true;
}

function verifyOTP(phone, enteredOTP) {
    const otpData = JSON.parse(sessionStorage.getItem('currentOTP') || '{}');
    
    if (!otpData.phone || otpData.phone !== phone) {
        throw new Error('ูู ูุชู ุฅุฑุณุงู ุฑูุฒ ุชุญูู ููุฐุง ุงูุฑูู');
    }
    
    if (new Date() > new Date(otpData.expiresAt)) {
        throw new Error('ุงูุชูุช ุตูุงุญูุฉ ุฑูุฒ ุงูุชุญูู');
    }
    
    if (otpData.otp !== enteredOTP) {
        throw new Error('ุฑูุฒ ุงูุชุญูู ุฎุงุทุฆ');
    }
    
    // Mark phone as verified
    const user = users.find(u => u.phone === phone);
    if (user) {
        user.isVerified = true;
        saveUsers();
    }
    
    // Clear OTP
    sessionStorage.removeItem('currentOTP');
    
    return true;
}

// Biometric authentication
function initBiometricAuth() {
    if (!window.PublicKeyCredential) {
        console.log('WebAuthn not supported');
        return;
    }
    
    // Check if biometric is available
    PublicKeyCredential.isUserVerifyingPlatformAuthenticatorAvailable()
        .then(available => {
            if (available) {
                console.log('Biometric authentication available');
                showBiometricOptions();
            }
        })
        .catch(err => {
            console.log('Biometric check failed:', err);
        });
}

function showBiometricOptions() {
    // Show biometric login option
    const biometricSection = document.querySelector('.biometric-login');
    if (biometricSection) {
        biometricSection.style.display = 'block';
    }
}

function registerBiometric() {
    if (!currentUser) {
        showNotification('ูุฌุจ ุชุณุฌูู ุงูุฏุฎูู ุฃููุงู', 'error');
        return;
    }
    
    const publicKeyCredentialCreationOptions = {
        challenge: new Uint8Array(32),
        rp: {
            name: "DeltaStars",
            id: window.location.hostname,
        },
        user: {
            id: new TextEncoder().encode(currentUser.id),
            name: currentUser.email || currentUser.phone,
            displayName: currentUser.name,
        },
        pubKeyCredParams: [{alg: -7, type: "public-key"}],
        authenticatorSelection: {
            authenticatorAttachment: "platform",
            userVerification: "required"
        },
        timeout: 60000,
        attestation: "direct"
    };
    
    navigator.credentials.create({
        publicKey: publicKeyCredentialCreationOptions
    }).then((credential) => {
        // Store credential info
        const credentialData = {
            id: credential.id,
            userId: currentUser.id,
            createdAt: new Date().toISOString()
        };
        
        let biometricCredentials = JSON.parse(localStorage.getItem('deltaStarsBiometric') || '[]');
        biometricCredentials.push(credentialData);
        localStorage.setItem('deltaStarsBiometric', JSON.stringify(biometricCredentials));
        
        showNotification('ุชู ุชุณุฌูู ุงูุจุตูุฉ ุจูุฌุงุญ', 'success');
    }).catch((err) => {
        console.error('Biometric registration failed:', err);
        showNotification('ูุดู ูู ุชุณุฌูู ุงูุจุตูุฉ', 'error');
    });
}

function loginWithBiometric() {
    const biometricCredentials = JSON.parse(localStorage.getItem('deltaStarsBiometric') || '[]');
    
    if (biometricCredentials.length === 0) {
        showNotification('ูุง ุชูุฌุฏ ุจุตูุฉ ูุณุฌูุฉ', 'warning');
        return;
    }
    
    const publicKeyCredentialRequestOptions = {
        challenge: new Uint8Array(32),
        allowCredentials: biometricCredentials.map(cred => ({
            id: Uint8Array.from(atob(cred.id), c => c.charCodeAt(0)),
            type: 'public-key'
        })),
        timeout: 60000,
        userVerification: "required"
    };
    
    navigator.credentials.get({
        publicKey: publicKeyCredentialRequestOptions
    }).then((credential) => {
        // Find user by credential
        const credId = btoa(String.fromCharCode(...new Uint8Array(credential.rawId)));
        const credData = biometricCredentials.find(c => c.id === credId);
        
        if (credData) {
            const user = users.find(u => u.id === credData.userId) || 
                        (credData.userId === 'admin' ? {
                            id: 'admin',
                            name: 'ูุฏูุฑ ุงููุธุงู',
                            role: 'admin',
                            email: ADMIN_CREDENTIALS.email,
                            phone: ADMIN_CREDENTIALS.phone
                        } : null);
            
            if (user) {
                setCurrentUser(user);
                
                // Close login modal
                const loginModal = bootstrap.Modal.getInstance(document.getElementById('loginModal'));
                if (loginModal) loginModal.hide();
                
                showNotification('ุชู ุชุณุฌูู ุงูุฏุฎูู ุจุงูุจุตูุฉ', 'success');
            }
        }
    }).catch((err) => {
        console.error('Biometric login failed:', err);
        showNotification('ูุดู ูู ุชุณุฌูู ุงูุฏุฎูู ุจุงูุจุตูุฉ', 'error');
    });
}

// User management
function setCurrentUser(user) {
    currentUser = user;
    localStorage.setItem('deltaStarsCurrentUser', JSON.stringify(user));
    updateUserInterface();
    showUserLoggedIn();
}

function logout() {
    currentUser = null;
    localStorage.removeItem('deltaStarsCurrentUser');
    updateUserInterface();
    showNotification('ุชู ุชุณุฌูู ุงูุฎุฑูุฌ', 'info');
}

function updateUserInterface() {
    const userDisplay = document.getElementById('userDisplay');
    const loginSection = document.getElementById('loginSection');
    const registerSection = document.getElementById('registerSection');
    const profileSection = document.getElementById('profileSection');
    const ordersSection = document.getElementById('ordersSection');
    const logoutSection = document.getElementById('logoutSection');
    const adminPanel = document.getElementById('adminPanel');
    
    if (currentUser) {
        if (userDisplay) userDisplay.textContent = currentUser.name;
        if (loginSection) loginSection.style.display = 'none';
        if (registerSection) registerSection.style.display = 'none';
        if (profileSection) profileSection.style.display = 'block';
        if (ordersSection) ordersSection.style.display = 'block';
        if (logoutSection) logoutSection.style.display = 'block';
        
        // Show admin panel if admin
        if (adminPanel && (currentUser.role === 'admin' || currentUser.role === 'developer')) {
            adminPanel.style.display = 'block';
        }
    } else {
        if (userDisplay) userDisplay.textContent = 'ุงูุญุณุงุจ';
        if (loginSection) loginSection.style.display = 'block';
        if (registerSection) registerSection.style.display = 'block';
        if (profileSection) profileSection.style.display = 'none';
        if (ordersSection) ordersSection.style.display = 'none';
        if (logoutSection) logoutSection.style.display = 'none';
        if (adminPanel) adminPanel.style.display = 'none';
    }
}

function showUserLoggedIn() {
    if (!currentUser) return;
    
    // Welcome message for new logins
    if (currentUser.lastLogin) {
        const lastLoginDate = new Date(currentUser.lastLogin);
        const now = new Date();
        const diffMinutes = (now - lastLoginDate) / (1000 * 60);
        
        if (diffMinutes > 30) { // If last login was more than 30 minutes ago
            showNotification(`ูุฑุญุจุงู ${currentUser.name}! ๐`, 'success');
        }
    }
    
    // Check for VIP status
    if (currentUser.accountType === 'vip') {
        showNotification('๐ ุฃููุงู ุจุนุถู VIP ุงููููุฒ!', 'info');
    }
}

// User profile management
function showProfile() {
    if (!currentUser) {
        showLogin();
        return;
    }
    
    const modal = document.createElement('div');
    modal.className = 'modal fade';
    modal.innerHTML = `
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">ุงูููู ุงูุดุฎุตู</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>
                <div class="modal-body">
                    <div class="profile-tabs">
                        <ul class="nav nav-tabs">
                            <li class="nav-item">
                                <button class="nav-link active" data-tab="info">ุงููุนูููุงุช ุงูุดุฎุตูุฉ</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" data-tab="addresses">ุงูุนูุงููู</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" data-tab="payments">ุทุฑู ุงูุฏูุน</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" data-tab="preferences">ุงูุชูุถููุงุช</button>
                            </li>
                            <li class="nav-item">
                                <button class="nav-link" data-tab="security">ุงูุฃูุงู</button>
                            </li>
                        </ul>
                        
                        <div class="tab-content mt-3" id="profileTabContent">
                            ${getProfileInfoTab()}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    `;
    
    document.body.appendChild(modal);
    
    const profileModal = new bootstrap.Modal(modal);
    profileModal.show();
    
    // Setup tab functionality
    setupProfileTabs(modal);
    
    modal.addEventListener('hidden.bs.modal', function() {
        modal.remove();
    });
}

