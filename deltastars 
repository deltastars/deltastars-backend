  // ูุชุญ ููุตุฉ ุงูุชูุงุตู
  async openPlatform(platformName) {
    const platform = this.platforms[platformName];
    
    if (!platform) {
      console.error('Platform not found:', platformName);
      return false;
    }

    try {
      // ูุญุงููุฉ ูุชุญ ุงูุชุทุจูู ุฃููุงู
      const deepLinkOpened = await this.openDeepLink(platform.deepLink);
      if (deepLinkOpened) return true;

      // ุฅุฐุง ูุดูุ ูุชุญ ุงููุชุตูุญ
      await Linking.openURL(platform.url);
      return true;
    } catch (error) {
      console.error(`Failed to open ${platformName}:`, error);
      return false;
    }
  }

  // ูุชุญ ุงูุฑุงุจุท ุงูุนููู
  async openDeepLink(deepLink) {
    try {
      const supported = await Linking.canOpenURL(deepLink);
      if (supported) {
        await Linking.openURL(deepLink);
        return true;
      }
      return false;
    } catch (error) {
      return false;
    }
  }

  // ูุดุงุฑูุฉ ุงููุญุชูู
  async shareContent(content) {
    const shareUrl = 'https://linktr.ee/deltastar6';
    
    const message = `
${content.title}

${content.description}

${shareUrl}

ุชุงุจุนูุง ุนูู:
${this.getSocialLinksText()}
    `.trim();

    // ูู ุงูุชุทุจูู ุงููุนููุ ุณูุณุชุฎุฏู Share ูู React Native
    return message;
  }

  // ุงูุญุตูู ุนูู ุฑูุงุจุท ูุณุงุฆู ุงูุชูุงุตู
  getSocialLinks() {
    return Object.entries(this.platforms).map(([key, platform]) => ({
      name: key,
      url: platform.url,
      icon: platform.icon,
      deepLink: platform.deepLink
    }));
  }
