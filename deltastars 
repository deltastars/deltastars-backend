    // Implementation for adding new address
    console.log('Add new address');
}

function addPaymentMethod() {
    // Implementation for adding payment method
    console.log('Add payment method');
}

function logoutAllDevices() {
    if (confirm('ูู ุฃูุช ูุชุฃูุฏ ูู ุชุณุฌูู ุงูุฎุฑูุฌ ูู ุฌููุน ุงูุฃุฌูุฒุฉุ')) {
        logout();
        showNotification('ุชู ุชุณุฌูู ุงูุฎุฑูุฌ ูู ุฌููุน ุงูุฃุฌูุฒุฉ', 'success');
    }
}

// Form event handlers
document.addEventListener('DOMContentLoaded', function() {
    initAuth();
    
    // Login form
    const loginForm = document.getElementById('loginForm');
    if (loginForm) {
        loginForm.addEventListener('submit', function(e) {
            e.preventDefault();
            
            const phone = document.getElementById('loginPhone').value;
            const password = document.getElementById('loginPassword').value;
            
            try {
                loginUser(phone, password);
                
                // Close modal
                const modal = bootstrap.Modal.getInstance(document.getElementById('loginModal'));
                if (modal) modal.hide();
                
                showNotification('ุชู ุชุณุฌูู ุงูุฏุฎูู ุจูุฌุงุญ', 'success');
                
            } catch (error) {
                showNotification(error.message, 'error');
            }
        });
    }
    
    // Register form
    const registerForm = document.getElementById('registerForm');
    if (registerForm) {
        registerForm.addEventListener('submit', function(e) {
            e.preventDefault();
            
            const formData = {
                name: document.getElementById('registerName').value,
                phone: document.getElementById('registerPhone').value,
                email: document.getElementById('registerEmail').value,
                password: document.getElementById('registerPassword').value
            };
            
            const confirmPassword = document.getElementById('confirmPassword').value;
            
            if (formData.password !== confirmPassword) {
                showNotification('ูููุชุง ุงููุฑูุฑ ุบูุฑ ูุชุทุงุจูุชูู', 'error');
                return;
            }
            
            try {
                registerUser(formData);
                
                // Close modal and show OTP
                const modal = bootstrap.Modal.getInstance(document.getElementById('registerModal'));
                if (modal) modal.hide();
                
                // Send OTP
                sendOTP(formData.phone);
                
                setTimeout(() => {
                    new bootstrap.Modal(document.getElementById('otpModal')).show();
                }, 300);
                
            } catch (error) {
                showNotification(error.message, 'error');
            }
        });
    }
    
    // OTP form
    const otpForm = document.getElementById('otpForm');
    if (otpForm) {
        otpForm.addEventListener('submit', function(e) {
            e.preventDefault();
            
            const phone = document.getElementById('registerPhone')?.value;
            const otp = document.getElementById('otpCode').value;
            
            try {
                verifyOTP(phone, otp);
                
                // Close modal
                const modal = bootstrap.Modal.getInstance(document.getElementById('otpModal'));
                if (modal) modal.hide();
                
                showNotification('ุชู ุงูุชุญูู ูู ุฑูู ุงููุงุชู ุจูุฌุงุญ', 'success');
                
                // Auto login
                const user = users.find(u => u.phone === phone);
                if (user) {
                    setCurrentUser(user);
                }
                
            } catch (error) {
                showNotification(error.message, 'error');
            }
        });
    }
});

// Forgot password
function showForgotPassword() {
    // Implementation for forgot password
    showNotification('ุณูุชู ุฅุถุงูุฉ ูุฐู ุงูููุฒุฉ ูุฑูุจุงู', 'info');
}

function resendOTP() {
    const phone = document.getElementById('registerPhone')?.value;
    if (phone) {
        sendOTP(phone);
        showNotification('ุชู ุฅุนุงุฏุฉ ุฅุฑุณุงู ุฑูุฒ ุงูุชุญูู', 'info');
    }
}

