                    <button type="button" class="btn btn-outline-secondary" onclick="upgradeToVIP()">
                        ${currentUser.accountType === 'vip' ? 'ุฃูุช ุนุถู VIP ุจุงููุนู' : 'ุชุฑููุฉ ุฅูู VIP'}
                    </button>
                </div>
            </form>
        </div>
    `;
}

function setupProfileTabs(modal) {
    const tabButtons = modal.querySelectorAll('[data-tab]');
    const tabContent = modal.querySelector('#profileTabContent');
    
    tabButtons.forEach(button => {
        button.addEventListener('click', function() {
            const tab = this.getAttribute('data-tab');
            
            // Update active tab
            tabButtons.forEach(btn => btn.classList.remove('active'));
            this.classList.add('active');
            
            // Update content
            switch (tab) {
                case 'info':
                    tabContent.innerHTML = getProfileInfoTab();
                    setupProfileForm();
                    break;
                case 'addresses':
                    tabContent.innerHTML = getAddressesTab();
                    break;
                case 'payments':
                    tabContent.innerHTML = getPaymentMethodsTab();
                    break;
                case 'preferences':
                    tabContent.innerHTML = getPreferencesTab();
                    break;
                case 'security':
                    tabContent.innerHTML = getSecurityTab();
                    break;
            }
        });
    });
    
    // Setup initial form
    setupProfileForm();
}

function setupProfileForm() {
    const form = document.getElementById('profileForm');
    if (form) {
        form.addEventListener('submit', function(e) {
            e.preventDefault();
            updateUserProfile();
        });
    }
}

function updateUserProfile() {
    if (!currentUser) return;
    
    const profileData = {
        name: document.getElementById('profileName').value,
        email: document.getElementById('profileEmail').value,
        birthdate: document.getElementById('profileBirthdate').value,
        gender: document.getElementById('profileGender').value
    };
    
    // Update current user
    Object.assign(currentUser, profileData);
    
    // Update in users array
    const userIndex = users.findIndex(u => u.id === currentUser.id);
    if (userIndex !== -1) {
        users[userIndex] = currentUser;
        saveUsers();
    }
    
    // Update localStorage
    localStorage.setItem('deltaStarsCurrentUser', JSON.stringify(currentUser));
    
    showNotification('ุชู ุชุญุฏูุซ ุงูููู ุงูุดุฎุตู', 'success');
}

function getAddressesTab() {
    const addresses = currentUser.addresses || [];
    
    return `
        <div class="addresses-section">
            <div class="d-flex justify-content-between align-items-center mb-3">
                <h6>ุนูุงูููู</h6>
                <button class="btn btn-primary btn-sm" onclick="addNewAddress()">
                    <i class="fas fa-plus"></i> ุฅุถุงูุฉ ุนููุงู ุฌุฏูุฏ
                </button>
            </div>
            
            ${addresses.length === 0 ? 
                '<p class="text-muted">ูุง ุชูุฌุฏ ุนูุงููู ูุญููุธุฉ</p>' :
                addresses.map((address, index) => `
                    <div class="address-card mb-3 p-3 border rounded">
                        <div class="d-flex justify-content-between">
                            <div>
                                <h6>${address.label || 'ุนููุงู ' + (index + 1)}</h6>
                                <p class="mb-1">${address.fullAddress}</p>
                                <p class="mb-1">${address.city}</p>
                                <small class="text-muted">${address.phone}</small>
                                ${address.isDefault ? '<span class="badge bg-success ms-2">ุงูุนููุงู ุงูุงูุชุฑุงุถู</span>' : ''}
                            </div>
                            <div class="dropdown">
                                <button class="btn btn-sm btn-outline-secondary dropdown-toggle" data-bs-toggle="dropdown">
                                    <i class="fas fa-ellipsis-v"></i>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a class="dropdown-item" href="#" onclick="editAddress(${index})">ุชุนุฏูู</a></li>
                                    <li><a class="dropdown-item" href="#" onclick="setDefaultAddress(${index})">ุฌุนู ุงูุชุฑุงุถู</a></li>
                                    <li><a class="dropdown-item text-danger" href="#" onclick="deleteAddress(${index})">ุญุฐู</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                `).join('')
            }
        </div>
    `;
}

