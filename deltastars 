services/aiService.js

```javascript
import axios from 'axios';

class AIService {
  // ุชุญููู ุณููู ุงูุนููุงุก
  async analyzeCustomerBehavior(customerId) {
    try {
      const response = await axios.get(`/api/ai/customer-analysis/${customerId}`);
      return response.data;
    } catch (error) {
      console.error('AI analysis failed:', error);
      return this.getDefaultAnalysis();
    }
  }

  // ุงูุชูุจุค ุจุงูุทูุจุงุช
  async predictOrders(timeframe = 'weekly') {
    try {
      const response = await axios.get('/api/ai/order-predictions', {
        params: { timeframe }
      });
      return response.data;
    } catch (error) {
      console.error('Order prediction failed:', error);
      return this.getDefaultPredictions();
    }
  }

  // ุชุญุณูู ุงูุฃุณุนุงุฑ
  async optimizePricing(productId, marketData) {
    try {
      const response = await axios.post('/api/ai/price-optimization', {
        productId,
        marketData
      });
      return response.data;
    } catch (error) {
      console.error('Price optimization failed:', error);
      return null;
    }
  }

  // ุงูุชุดุงู ุงูุดุฐูุฐ
  async detectAnomalies(dataType) {
    try {
      const response = await axios.get('/api/ai/anomaly-detection', {
        params: { dataType }
      });
      return response.data;
    } catch (error) {
      console.error('Anomaly detection failed:', error);
      return [];
    }
  }

  // ุงูุชูุตูุงุช ุงูุฐููุฉ
  async getSmartRecommendations(userId, context) {
    try {
      const response = await axios.post('/api/ai/recommendations', {
        userId,
        context
      });
      return response.data;
    } catch (error) {
      console.error('Recommendations failed:', error);
      return this.getDefaultRecommendations();
    }
  }

  // ุงูุชุญูููุงุช ุงูุชูุจุคูุฉ ูููุฎุฒูู
  async predictInventoryNeeds() {
    try {
      const response = await axios.get('/api/ai/inventory-predictions');
      return response.data;
    } catch (error) {
      console.error('Inventory prediction failed:', error);
      return this.getDefaultInventoryPredictions();
    }
  }

  getDefaultAnalysis() {
    return {
      purchaseFrequency: 'medium',
      preferredCategories: ['fruits', 'vegetables'],
      averageOrderValue: 250,
      potentialUpsell: true,
      retentionScore: 0.75
    };
  }

  getDefaultPredictions() {
    return {
      predictedOrders: 150,
      confidence: 0.85,
      trendingProducts: ['ุชูุงุญ', 'ููุฒ', 'ุทูุงุทู'],
      seasonalTrends: {
        upcoming: ['ุชูุฑ', 'ุฑูุงู'],
        declining: ['ุฎูุงุฑ', 'ุฎุณ']
      }
    };
  }

  getDefaultRecommendations() {
    return {
      products: ['ุชูุงุญ ูุงุฎุฑ', 'ุชูุฑ ูุฌุฏูู', 'ุฒูุชูู ุนุถูู'],
      bundles: ['ุณูุฉ ุงูููุงูู ุงูููุณููุฉ', 'ุจุงูุฉ ุงูุฎุถุฑูุงุช ุงูุทุงุฒุฌุฉ'],
      promotions: ['ุฎุตู 10% ุนูู ุงูุทูุจุงุช ููู 500 ุฑูุงู']
    };
  }

  getDefaultInventoryPredictions() {
    return {
      needRestock: ['ุชูุงุญ', 'ููุฒ', 'ุจุตู'],
      overstock: ['ุฌุฒุฑ', 'ููุช'],
      predictedShortages: [
        { product: 'ุทูุงุทู', daysUntilShortage: 3, urgency: 'high' },
        { product: 'ุฎูุงุฑ', daysUntilShortage: 5, urgency: 'medium' }
      ]
    };
  }
}

export default new AIService();
```

