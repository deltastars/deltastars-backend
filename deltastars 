function getOrderReviewStep() {
    let subtotal = 0;
    const orderItems = cart.map(item => {
        const product = getProductById(item.id);
        const itemTotal = product.price * item.quantity;
        subtotal += itemTotal;
        return { ...product, quantity: item.quantity, itemTotal };
    });
    
    const shipping = subtotal > 100 ? 0 : 15;
    const tax = Math.round(subtotal * 0.15);
    const total = subtotal + shipping + tax;
    
    return `
        <div class="order-review">
            <h5>ูุฑุงุฌุนุฉ ุงูุทูุจ</h5>
            
            <div class="order-summary">
                <h6>ุชูุงุตูู ุงูุทูุจ</h6>
                <div class="order-items">
                    ${orderItems.map(item => `
                        <div class="order-item">
                            <img src="${item.image}" alt="${item.name}" class="item-thumb">
                            <div class="item-details">
                                <span class="item-name">${item.name}</span>
                                <span class="item-qty">ุงููููุฉ: ${item.quantity}</span>
                            </div>
                            <span class="item-price">${item.itemTotal} ุฑูุงู</span>
                        </div>
                    `).join('')}
                </div>
                
                <div class="order-totals">
                    <div class="total-row">
                        <span>ุงููุฌููุน ุงููุฑุนู:</span>
                        <span>${subtotal} ุฑูุงู</span>
                    </div>
                    <div class="total-row">
                        <span>ุงูุดุญู:</span>
                        <span>${shipping === 0 ? 'ูุฌุงูู' : shipping + ' ุฑูุงู'}</span>
                    </div>
                    <div class="total-row">
                        <span>ุงูุถุฑูุจุฉ (15%):</span>
                        <span>${tax} ุฑูุงู</span>
                    </div>
                    <div class="total-row total-final">
                        <span><strong>ุงููุฌููุน ุงูููู:</strong></span>
                        <span><strong>${total} ุฑูุงู</strong></span>
                    </div>
                </div>
            </div>
            
            <div class="delivery-summary mt-4">
                <h6>ูุนูููุงุช ุงูุชูุตูู</h6>
                <div id="deliverySummary"></div>
            </div>
            
            <div class="payment-summary mt-3">
                <h6>ุทุฑููุฉ ุงูุฏูุน</h6>
                <div id="paymentSummary"></div>
            </div>
            
            <div class="terms-agreement mt-4">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" id="agreeTerms" required>
                    <label class="form-check-label" for="agreeTerms">
                        ุฃูุงูู ุนูู <a href="#" onclick="showTerms()">ุงูุดุฑูุท ูุงูุฃุญูุงู</a> ู 
                        <a href="#" onclick="showPrivacyPolicy()">ุณูุงุณุฉ ุงูุฎุตูุตูุฉ</a>
                    </label>
                </div>
            </div>
            
            <div class="d-flex justify-content-between mt-4">
                <button type="button" class="btn btn-secondary" onclick="showCheckoutStep(2)">
                    ุงูุณุงุจู
                </button>
                <button type="button" class="btn btn-success btn-lg" onclick="placeOrder()" id="placeOrderBtn">
                    <i class="fas fa-check"></i> ุชุฃููุฏ ุงูุทูุจ
                </button>
            </div>
        </div>
    `;
}

function showCheckoutStep(step) {
    const content = document.getElementById('checkoutContent');
    const steps = document.querySelectorAll('.step');
    
    // Update step indicators
    steps.forEach((stepEl, index) => {
        stepEl.classList.toggle('active', index + 1 === step);
    });
    
    // Show appropriate content
    switch (step) {
        case 1:
            content.innerHTML = getDeliveryInfoStep();
            setupDeliveryForm();
            break;
        case 2:
            content.innerHTML = getPaymentMethodStep();
            setupPaymentForm();
            break;
        case 3:
            content.innerHTML = getOrderReviewStep();
            populateOrderReview();
            break;
    }
}

function setupDeliveryForm() {
    const form = document.getElementById('deliveryForm');
    form.addEventListener('submit', function(e) {
        e.preventDefault();
        
        // Validate form
        if (!form.checkValidity()) {
            form.classList.add('was-validated');
            return;
        }
        
        // Store delivery info
        const deliveryInfo = {
            fullName: document.getElementById('fullName').value,
            phone: document.getElementById('phone').value,
            address: document.getElementById('address').value,
            city: document.getElementById('city').value,
            postalCode: document.getElementById('postalCode').value,
            deliveryNotes: document.getElementById('deliveryNotes').value,
            enableGPS: document.getElementById('enableGPS').checked
        };
        
        sessionStorage.setItem('checkoutDelivery', JSON.stringify(deliveryInfo));
        
        // Request location if GPS enabled
        if (deliveryInfo.enableGPS) {
            requestUserLocation();
        }
        
        showCheckoutStep(2);
    });
}

function setupPaymentForm() {
    const paymentOptions = document.querySelectorAll('input[name="paymentMethod"]');
    paymentOptions.forEach(option => {
        option.addEventListener('change', function() {
            const bankDetails = document.getElementById('bankDetails');
            if (bankDetails) {
                bankDetails.style.display = this.value === 'bank_transfer' ? 'block' : 'none';
            }
        });
    });
}

function populateOrderReview() {
    const deliveryInfo = JSON.parse(sessionStorage.getItem('checkoutDelivery') || '{}');
    const paymentMethod = document.querySelector('input[name="paymentMethod"]:checked')?.value || 'bank_transfer';
    
    // Populate delivery summary
    const deliverySummary = document.getElementById('deliverySummary');
    if (deliverySummary) {
        deliverySummary.innerHTML = `
            <p><strong>ุงูุงุณู:</strong> ${deliveryInfo.fullName}</p>
            <p><strong>ุงููุงุชู:</strong> ${deliveryInfo.phone}</p>
            <p><strong>ุงูุนููุงู:</strong> ${deliveryInfo.address}</p>
            <p><strong>ุงููุฏููุฉ:</strong> ${getCityName(deliveryInfo.city)}</p>
            ${deliveryInfo.deliveryNotes ? `<p><strong>ููุงุญุธุงุช:</strong> ${deliveryInfo.deliveryNotes}</p>` : ''}
        `;
    }
    
    // Populate payment summary
    const paymentSummary = document.getElementById('paymentSummary');
    if (paymentSummary) {
        const paymentMethods = {
            'bank_transfer': 'ุชุญููู ุจููู',
            'cod': 'ุงูุฏูุน ุนูุฏ ุงูุชูุตูู',
            'mada': 'ุจุทุงูุฉ ูุฏู / ููุฒุง',
            'stc_pay': 'STC Pay'
        };
        paymentSummary.innerHTML = `<p>${paymentMethods[paymentMethod]}</p>`;
    }
}

function getCityName(cityCode) {
    const cities = {
        'riyadh': 'ุงูุฑูุงุถ',
        'jeddah': 'ุฌุฏุฉ',
        'dammam': 'ุงูุฏูุงู',
        'mecca': 'ููุฉ ุงูููุฑูุฉ',
        'medina': 'ุงููุฏููุฉ ุงููููุฑุฉ',
        'other': 'ุฃุฎุฑู'
    };
    return cities[cityCode] || cityCode;
}

function requestUserLocation() {
    if (!navigator.geolocation) {
        showNotification('ูุชุตูุญู ูุง ูุฏุนู ุฎุฏูุฉ ุชุญุฏูุฏ ุงููููุน', 'warning');
        return;
    }
    
    navigator.geolocation.getCurrentPosition(
        function(position) {
            const location = {
                latitude: position.coords.latitude,
                longitude: position.coords.longitude,
                accuracy: position.coords.accuracy,
                timestamp: new Date().toISOString()
            };
            
            sessionStorage.setItem('checkoutLocation', JSON.stringify(location));
            showNotification('ุชู ุชุญุฏูุฏ ูููุนู ุจูุฌุงุญ', 'success');
        },
        function(error) {
            console.log('Location error:', error);
            showNotification('ูุง ูููู ุชุญุฏูุฏ ูููุนู ุงูุญุงูู', 'warning');
        },
        {
            enableHighAccuracy: true,
            timeout: 10000,
            maximumAge: 60000
        }
    );
}

function copyBankDetails() {
    const details = `
ุงุณู ุงูุจูู: ${COMPANY_BANK_DETAILS.bankName}
ุงุณู ุงูุญุณุงุจ: ${COMPANY_BANK_DETAILS.accountName}
ุฑูู ุงูุญุณุงุจ: ${COMPANY_BANK_DETAILS.accountNumber}
ุงูุขูุจุงู: ${COMPANY_BANK_DETAILS.iban}
ุฑูู ุงููุฑุน: ${COMPANY_BANK_DETAILS.branchCode}
    `.trim();
    
    navigator.clipboard.writeText(details).then(() => {
        showNotification('ุชู ูุณุฎ ุชูุงุตูู ุงูุญุณุงุจ', 'success');
    }).catch(() => {
        showNotification('ูุดู ูู ูุณุฎ ุงูุชูุงุตูู', 'error');
    });
}

