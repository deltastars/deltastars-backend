          window.APPLET_FILES = ["index.tsx","metadata.json","index.html","components/App.tsx","types.ts","constants.ts","components/lib/Header.tsx","components/Footer.tsx","components/ProductCard.tsx","components/lib/Home.tsx","components/ProductsPage.tsx","components/CartPage.tsx","components/lib/Icons.tsx","components/LoginPage.tsx","components/DashboardPage.tsx","contexts/I18nContext.tsx","components/translations.ts","components/vip/index.html","components/vip/index.tsx","components/vip/VipApp.tsx","components/vip/VipLoginPage.tsx","components/vip/VipDashboard.tsx","components/products.ts","components/supabaseClient.ts","components/LoadingSpinner.tsx","components/VipLoginPage.tsx","components/VipDashboardPage.tsx","components/lib/webAuthn.ts","components/ShowroomPage.tsx","components/lib/deviceTrust.ts","components/accounting.ts","components/AccountsView.tsx","components/PasswordGuide.tsx","manifest.json","service-worker.js","components/SectionAuthModal.tsx","components/ProductDetailPage.tsx","WishlistPage.tsx","components/lib/api.ts","components/lib/mockApi.ts","components/ErrorBoundary.tsx","components/AiAssistant.tsx","contexts/ToastContext.tsx","components/Toast.tsx","components/ToastContainer.tsx","components/WishlistPage.tsx","lib/mockApi.ts"];
